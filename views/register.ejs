<% include ./partials/header %>

<form action="/register" method="POST">
    <div class="container login-form">
        <div class="form-group">
            <label>Email</label>
    <input class="form-control" type="text" name="username" placeholder="Please choose a username...">
    </div>
    <div class="form-group">
        <label>Password</label>
    <input class="form-control" type="password" name="password" placeholder="Please choose a password...">
    </div>
    <button class="btn btn-primary">SignUp!</button>
    </div>
</form>

